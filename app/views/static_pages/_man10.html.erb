<h1><%= @weight.max %> lbs Bracket</h1>
<h3>Pool 1</h3>
<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<th>Name</th>
			<th>R1</th>
			<th>R2</th>
			<th>R3</th>
			<th>R4</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td>1. <%= @seed1.name %> <%= @seed1.season_win %>-<%= @seed1.season_loss %></td>
			<td>V2</td>
			<td>V3</td>
			<td>V5</td>
			<td>V4</td>
			<td>BYE</td>
		</tr>
		<tr>
			<td>2. <%= @seed10.name %> <%= @seed10.season_win %>-<%= @seed10.season_loss %></td>
